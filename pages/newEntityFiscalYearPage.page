<apex:page standardController="EntityFiscalyear__c" >
  <apex:form >
    <apex:pageBlock title="Entity Fiscal Year" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! EntityFiscalyear__c.Entity__c}" required="true"/>  
        <apex:inputField value="{! EntityFiscalyear__c.FiscalYearEndDate__c}" required="true"/>
        <apex:inputField value="{! EntityFiscalyear__c.EntityFiscalYearEID__c}" />      
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>