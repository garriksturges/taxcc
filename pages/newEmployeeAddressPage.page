<apex:page standardController="EmployeeAddress__c" >
  <apex:form >
    <apex:pageBlock title="Employee Address" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{!EmployeeAddress__c.Employee__c}" required="true"/>
        <apex:inputField value="{!EmployeeAddress__c.StreetAddressText__c}" required="true"/>
        <apex:inputField value="{!EmployeeAddress__c.CityName__c}" required="true"/>
        <apex:inputField value="{!EmployeeAddress__c.State__c}" />
        <apex:inputField value="{!EmployeeAddress__c.ZipCode__c}" required="true"/>        
        <apex:inputField value="{!EmployeeAddress__c.County__c}" />
        <apex:inputField value="{!EmployeeAddress__c.EffectiveDate__c}" />
        <apex:inputField value="{!EmployeeAddress__c.Latitude__c}" />
        <apex:inputField value="{!EmployeeAddress__c.Longitude__c}" />
        <apex:inputField value="{!EmployeeAddress__c.IE2EmployeeAddressId__c}" />
        <apex:inputField value="{!EmployeeAddress__c.EmployeeAddressEID__c}" />
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>