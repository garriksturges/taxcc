<apex:page standardController="ProgramCategoryPeriod__c" >
  <apex:form >
    <apex:pageBlock title="Program Category Period" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! ProgramCategoryPeriod__c.ProgramCategory__c}" required="true"/>  
        <apex:inputField value="{! ProgramCategoryPeriod__c.Area__c}" required="true"/>  
        <apex:inputField value="{! ProgramCategoryPeriod__c.StartDate__c}" required="true"/>  
        <apex:inputField value="{! ProgramCategoryPeriod__c.EndDate__c}" required="false"/>  
        <apex:inputField value="{! ProgramCategoryPeriod__c.IE2TeaID__c}" required="false"/>  
        <apex:inputField value="{! ProgramCategoryPeriod__c.ProgramCategoryPeriodEID__c}" required="false"/>  
           
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>