<apex:page standardController="EntityStructure__c" >
  <apex:form >
    <apex:pageBlock title="Entity Structure" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! EntityStructure__c.EntityName__c}" required="true"/>  
        <apex:inputField value="{! EntityStructure__c.EntityType__c}" />
        <apex:inputField value="{! EntityStructure__c.FederalTaxId__c}" />
        <apex:inputField value="{! EntityStructure__c.FederalEntityStartDate__c}" />
        <apex:inputField value="{! EntityStructure__c.FederalEntityEndDate__c}" />
        <apex:inputField value="{! EntityStructure__c.StateTaxId__c}" />
        <apex:inputField value="{! EntityStructure__c.StateEntityStartDate__c}" />
        <apex:inputField value="{! EntityStructure__c.StateEntityEndDate__c}" />
        <apex:inputField value="{! EntityStructure__c.StreetAddressText__c}" />
        <apex:inputField value="{! EntityStructure__c.CityName__c}" />
        <apex:inputField value="{! EntityStructure__c.State__c}" />
        <apex:inputField value="{! EntityStructure__c.ZipCode__c}" />
        <apex:inputField value="{! EntityStructure__c.County__c}" />
        <apex:inputField value="{! EntityStructure__c.EntityStructureEID__c}" />      
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>