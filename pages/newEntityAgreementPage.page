<apex:page standardController="EntityAgreement__c" >
  <apex:form >
    <apex:pageBlock title="Entity Agreement" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! EntityAgreement__c.Entity__c}" required="true"/>
        <apex:inputField value="{! EntityAgreement__c.ContractType__c}" required="true"/>
        <apex:inputField value="{! EntityAgreement__c.StartDate__c}"/>
        <apex:inputField value="{! EntityAgreement__c.EndDate__c}"/>
        <apex:inputField value="{! EntityAgreement__c.StreetAddressText__c}"/>
        <apex:inputField value="{! EntityAgreement__c.CityText__c}"/>
        <apex:inputField value="{! EntityAgreement__c.State__c}"/>
        <apex:inputField value="{! EntityAgreement__c.ZipCode__c}"/>
        <apex:inputField value="{! EntityAgreement__c.Agency__c}"/>
        
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>