<apex:page standardController="Wage__c" >
  <apex:form >
    <apex:pageBlock title="Wage" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! Wage__c.StartDate__c}" required="true"/>
        <apex:inputField value="{! Wage__c.EndDate__c}" required="true"/>
        <apex:inputField value="{! Wage__c.WageAmount__c}" required="true"/>
        <apex:inputField value="{! Wage__c.EmploymentPeriod__c}" required="true"/>
        <apex:inputField value="{! Wage__c.IE2WageID__c}" required="false"/>
        <apex:inputField value="{! Wage__c.WageEID__c}" required="false"/>
        
        
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>