<apex:page standardController="Credit__c" >
  <apex:form >
    <apex:pageBlock title="Credit" mode="edit">
      
<apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!save}" value="Save"/>
        <apex:commandButton action="{!cancel}" value="Cancel"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="" columns="1">
        <apex:inputField value="{! Credit__c.ProgramQualification__c}" required="true"/>
        <apex:inputField value="{! Credit__c.CreditAmount__c}" />
        <apex:inputField value="{! Credit__c.EndDate__c}" />
        <apex:inputField value="{! Credit__c.ProgramCategory__c}" />
        <apex:inputField value="{! Credit__c.StartDate__c}" />
        <apex:inputField value="{! Credit__c.EmploymentPeriodGenerated__c}" />
        <apex:inputField value="{! Credit__c.CreditEID__c}" />
      </apex:pageBlockSection>
    </apex:pageBlock>
  </apex:form>
</apex:page>